/*
* Initial release v1.0.0
* 1.0.1 Update to improve device support + Cleanup
* 1.0.2
* 1.0.3
* 1.0.4 Improved handeling of Govee Device Additions
* 1.0.5 Correct but that prevented device Additions if both Lights and appliances where added together.
* 1.0.6 Updated process for appliance device creation to allow for devices without gear commands.
* 1.0.7 Enhanced Logging, Added rate limit tracking to app, Enhanced non RGB device handeling
* 1.0.10 Correct failed update to github to include updates for new drivers
* 1.0.11 Added command verification methods in the Appliance drivers.
* 1.0.12
* 1.0.13 Corrected detection for White Light only driver
* 1.0.14 Added Notification for approaching rate limits for cloud API's
* 1.0.15 Found bug with how Light drivers were updating certain states
* 1.0.16 Updated Polling setup to allow value of 0
*     Added Actuator to Appliance devices to allow for better RM integration
* 1.0.19 Added ability for app to udpate API Key on child devices
* 1.0.20 Added Scenes and respective tools
*/

import groovy.json.JsonSlurper
import groovy.transform.Field

preferences
{
    page(name: 'mainPage', title: 'Govee Integration')
    page(name: 'deviceSelect', title: 'Select Light, Switch, Plug devices')
    page(name: 'deviceSelect2', title: 'Select Appliances')
    page(name: 'sceneExtract', title: 'Extract scenes')
    page(name: 'sceneExtract2', title: 'Govee Home Creds')
    page(name: 'about', title: 'About')
}

def initialize() {
    state.loggingLevelIDE = (settings.configLoggingLevelIDE) ? settings.configLoggingLevelIDE.toInteger() : 3
//    state.diyEffects = [:]
    lightEffectSetup()
    List child = getChildDevices()
    List childDNI = child.deviceNetworkId
    if (settings.APIKey != state.APIKey ) {
        child.each {
            logger('initialize() API key has been updated. Calling child devices to udpate', 'debug')
            it.initialize()
        }
        state?.APIKey = settings.APIKey
    }
    if (goveeDev) {
        def  goveeAdd = settings.goveeDev - child.label
        logger("initialize() Child device ${child} DNI's ${childDNI}", 'debug')
        logger("initialize() Govee Light/Switch/Plugs to add ${goveeAdd}.", 'info')
        if (goveeAdd) {
            def devices = goveeAdd
            logger("initialize() $devices are selcted to be integrated", 'info')
            logger('initialize() DEVICE INFORMATION', 'info')
            def params = [
            uri   : 'https://developer-api.govee.com',
            path  : '/v1/devices',
            headers: ['Govee-API-Key': settings.APIKey, 'Content-Type': 'application/json'],
        ]

            try {
                httpGet(params) { resp ->
                    //                logger (${resp.data},"debug")
                    resp.data.data.devices.each {
                        deviceID = it.device
                        deviceModel = it.model
                        deviceName = it.deviceName
                        controllable = it.controllable
                        retrievable = it.retrievable
                        supportCmds = it.supportCmds
                        if (supportCmds.contains('colorTem') && supportCmds.contains('color')) {
                            ctMin = it.properties.colorTem.range.min
                            ctMax = it.properties.colorTem.range.max
                            devices.findAll { it == deviceName } .each {
                                logger("initialize()  configuring ${deviceName}", 'info')
                                addChildDevice('Mavrrick', 'Govee Lights, Plugs, and Switches Driver', "${deviceID}" , location.hubs[0].id, [
                                'name': 'Govee Light_Plug_Switch',
                                'label': deviceName,
                                'data': [
                                'deviceID': deviceID,
                                'deviceModel': deviceModel,
                                'retrievable': retrievable,
                                'apiKey': settings.APIKey,
                                'commands': supportCmds,
                                'ctMin': ctMin,
                                'ctMax': ctMax
                            ],
                    'completedSetup': true,
                ])
                        }
                    }
                    else if (supportCmds.contains('brightness')) {
                            ctMin = 'N/A'
                            ctMax = 'N/A'
                            devices.findAll { it == deviceName } .each {
                                logger("initialize()  configuring ${deviceName}", 'info')
                                addChildDevice('Mavrrick', 'Govee White Light Driver', "${deviceID}" , location.hubs[0].id, [
                                'name': 'Govee Light_Plug_Switch',
                                'label': deviceName,
                                'data': [
                                'deviceID': deviceID,
                                'deviceModel': deviceModel,
                                'retrievable': retrievable,
                                'apiKey': settings.APIKey,
                                'commands': supportCmds,
                                'ctMin': ctMin,
                                'ctMax': ctMax
                            ],
                    'completedSetup': true,
                ])
                    }
                }
                    else {
                            ctMin = 'N/A'
                            ctMax = 'N/A'
                            devices.findAll { it == deviceName } .each {
                                logger("initialize()  configuring ${deviceName}", 'info')
                                addChildDevice('Mavrrick', 'Govee Plugs and Switches Driver', "${deviceID}" , location.hubs[0].id, [
                                'name': 'Govee Light_Plug_Switch',
                                'label': deviceName,
                                'data': [
                                'deviceID': deviceID,
                                'deviceModel': deviceModel,
                                'retrievable': retrievable,
                                'apiKey': settings.APIKey,
                                'commands': supportCmds,
                                'ctMin': ctMin,
                                'ctMax': ctMax
                            ],
                    'completedSetup': true,
                ])
                    }
            }
        }
    }
    } catch (groovyx.net.http.HttpResponseException e) {
                logger("initialize() Error: e.statusCode ${e.statusCode}", 'error')
                logger("initialize() ${e}", 'error')

                return 'unknown'
}
            state?.installDev = goveeDev
            logger('initialize() Govee devices integrated', 'info')
                }
        }
          else {
        logger('initialize() No devices to add', 'info')
          }
    // add appliances
    if (goveeApp) {
        def  goveeAdd2 = settings.goveeApp - child.label
        logger("initialize() Govee Appliances to add ${goveeAdd2}.", 'info')
        if (goveeAdd2) {
            def devices = goveeAdd2
            logger("initialize() $devices are selcted to be integrated", 'info')
            logger('initialize() Appliance INFORMATION', 'info')
            def params2 = [
            uri   : 'https://developer-api.govee.com',
            path  : '/v1/appliance/devices',
            headers: ['Govee-API-Key': settings.APIKey, 'Content-Type': 'application/json'],
        ]

            try {
                httpGet(params2) { resp ->
                    //List each device assigned to current API key
                    //               logger ("initialize()" + ${resp.data},"debug")
                    resp.data.data.devices.each {
                        deviceID = it.device
                        deviceModel = it.model
                        deviceName = it.deviceName
                        controllable = it.controllable
                        retrievable = it.retrievable
                        supportCmds = it.supportCmds
                        modes = it.properties.mode.options
                        if (supportCmds.contains('gear')) {
                            gear = it.properties.gear.options.value
                            logger("initialize() $deviceName    Address: $deviceID    Model: $deviceModel retrievable: $retrievable controllable: $controllable modes: $modes gears: $gear", 'debug')
                            devices.findAll { it == deviceName } .each {
                                logger("initialize()  configuring ${deviceName}", 'info')
                                addChildDevice('Mavrrick', 'Govee Appliance Driver', "${deviceID}" , location.hubs[0].id, [
                                'name': 'Govee Appliance',
                                'label': deviceName,
                                'data': [
                                'deviceID': deviceID,
                                'deviceModel': deviceModel,
                                // Appliances are not retrieveable   'retrievable': retrievable,
                                'apiKey': settings.APIKey,
                                'commands': supportCmds,
                                'modes' : modes,
                                'gears' : gear
                            ],
                    'completedSetup': true,
                ])
                        }
                    }
                    else {
                            gear = 'N/A'
                            logger("initialize() $deviceName    Address: $deviceID    Model: $deviceModel retrievable: $retrievable controllable: $controllable modes: $modes gears: $gear", 'debug')
                            devices.findAll { it == deviceName } .each {
                                logger("initialize()  configuring ${deviceName}", 'info')
                                addChildDevice('Mavrrick', 'Govee Heating Appliance Driver', "${deviceID}" , location.hubs[0].id, [
                                'name': 'Govee Appliance',
                                'label': deviceName,
                                'data': [
                                'deviceID': deviceID,
                                'deviceModel': deviceModel,
                                // Appliances are not retrieveable   'retrievable': retrievable,
                                'apiKey': settings.APIKey,
                                'commands': supportCmds,
                                'modes' : modes,
                                'gears' : gear
                            ],
                    'completedSetup': true,
                ])
                    }
                }
            }
        }
    } catch (groovyx.net.http.HttpResponseException e) {
                logger("initialize() Error: e.statusCode ${e.statusCode}", 'error')
                logger("initialize() ${e}", 'error')

                return 'unknown'
    }
            state?.installedApp = goveeApp
            logger('initialize() Govee devices integrated', 'info')
                }
    }
          else {
        logger('initialize() No appliances to add', 'info')
          }
}

/*
    mainPage

    UI Page: Main menu for the app.
*/
def mainPage() {
    List child = getChildDevices()
    def childCount = child.size()
    dynamicPage(name: 'mainPage', title: 'Govee integration Main menu', uninstall: true, install: true, submitOnChange: true)
    {
        section('API Configuration  ')
        {
            paragraph "The Govee APi is required for this integration. Please obtain this value befor moving forward. You can obtain the Govee APi from the Govee Home app 'About Us' section."
            input 'APIKey', 'string', title: 'Enter Your API Key', required: true
        }
        section('Govee Device Add Page') {
            href 'deviceSelect', title: 'Device selection', description: 'Select Govee devices to add to your environment'
        }
        section('Govee Apliance Add Page') {
            href 'deviceSelect2', title: 'Device selection', description: 'Select Govee devices to add to your environment'
        }
        section('Current Integrated Device Information') {
            paragraph "There are ${childCount} devices integrated"
            paragraph "Your current integrated devices are ${child.label}"
                if (childCount > 0) { def maxPoll = 86400 / ((10000 - (50 * childCount)) / childCount).toDouble()
                maxPoll = maxPoll.round(1)
                paragraph "With the number of devices have you can poll as frequently as every ${maxPoll} seconds" }
            paragraph "You have ${state.dailyLimit} of your 10k Daily APIv1 Calls left."
            paragraph "You have ${state.dailyAppLimit} of your 100 Daily APIv2 Calls left."
        }
        section('Notification Options') {
            input 'notifyEnabled', 'bool', title: 'Enable Notification', required: false, defaultValue: false
            input name: 'notificationDevice', type: 'capability.notification', title: 'Send notification of rate limit consumption :', multiple: true
            input 'apiV1threshold', 'number', title: 'Daily Rate Limit threshold to send out notification for Lights, Switches, and Plugs', required: false , range: '1..500', defaultValue: 3
            input 'apiV2threshold', 'number', title: 'Daily Rate Limit threshold to send out notification for Appliances', required: false , range: '1..100', defaultValue: 3
        }
        section('Govee Scene Extract') {
            href 'sceneExtract', title: 'Scene Extract menu', description: 'Extract Scenes through this menu'
            paragraph "Click button below to scenes to all children device"  
            input "pushScenesUpdate" , "button",  title: "Click here to update all devices awareness of scenes"
            paragraph "Click button below to reload preload scenes"            
            input "sceneInitialize" , "button",  title: "Click here to reload preloaded scene information"
            paragraph "Click button below to clear DIY scenes"            
            input "sceneDIYInitialize" , "button",  title: "Click here to clear DIY scene information"
        }        
        section('Logging Options') {
            input(
                name: 'configLoggingLevelIDE',
                title: 'IDE Live Logging Level:\nMessages with this level and higher will be logged to the IDE.',
                type: 'enum',
                options: [
                    '0' : 'None',
                    '1' : 'Error',
                    '2' : 'Warning',
                    '3' : 'Info',
                    '4' : 'Debug',
                    '5' : 'Trace'
                ],
                defaultValue: '1',
                displayDuringSetup: true,
                required: false
            )
        }
        section('About') {
            href 'about', title: 'About Information menu', description: 'About Menu for Govee Integration'
        }
    }
}

def deviceSelect() {
    def options = [:]

    logger('deviceSelect() DEVICE INFORMATION', 'debug')
    def params = [
            uri   : 'https://developer-api.govee.com',
            path  : '/v1/devices',
            headers: ['Govee-API-Key': settings.APIKey, 'Content-Type': 'application/json'],
        ]

    try {
        httpGet(params) { resp ->
                //List each device assigned to current API key
                //                logger ("deviceSelect()" + ${resp.data},"debug")
                resp.data.data.devices.each {
                    deviceID = it.device
                    deviceName = it.deviceName
                    logger("deviceSelect() $deviceName    Address: $deviceID    Model: $deviceModel retrievable: $retrievable controllable: $controllable Color Temp: $ctMin Color Temp: $ctMax", 'debug')
                    options["${deviceName}"] = deviceName
                }
                logger(" deviceSelect() $options", 'debug')
        }
    } catch (groovyx.net.http.HttpResponseException e) {
        logger("deviceSelect() Error: e.statusCode ${e.statusCode}", 'error')
        logger("deviceSelect() ${e}", 'error')

        return 'unknown'
    }

    dynamicPage(name: 'deviceSelect', title: 'Add Devices page', uninstall: false, install: false)
    {
        section('Please select the Lights, Plugs, Switch you want to integrate')
        {
            paragraph 'Please select the devices you wish to integrate.'
            input(name: 'goveeDev', type: 'enum', required:false, description: 'Please select the devices you wish to integrate.', multiple:true,
                options: options, width: 8, height: 1)
        }
    }
}

def deviceSelect2() {
    def options2 = [:]
    log.debug 'Appliance INFORMATION'
    def paramsApp = [
            uri   : 'https://developer-api.govee.com',
            path  : '/v1/appliance/devices',
            headers: ['Govee-API-Key': settings.APIKey, 'Content-Type': 'application/json'],
        ]

    try {
        httpGet(paramsApp) { resp ->
                //List each device assigned to current API key
                //                logger ("deviceSelect2()" + ${resp.data},"debug")
                resp.data.data.devices.each {
                    deviceID = it.device
                    deviceName = it.deviceName
                    logger("deviceSelect2() $deviceName    Address: $deviceID    Model: $deviceModel controllable: $controllable modes: $modes", 'debug')
                    options2["${deviceName}"] = deviceName
                }
                logger("deviceSelect2() $options2", 'debug')
        }
    } catch (groovyx.net.http.HttpResponseException e) {
        logger("deviceSelect2() Error: e.statusCode ${e.statusCode}", 'error')
        logger("deviceSelect2() ${e}", 'error')

        return 'unknown'
    }
    dynamicPage(name: 'deviceSelect', title: 'Add Devices page', uninstall: false, install: false)
    {
        section('Please select the Govee Appliances you want to integrate')
        {
            paragraph 'Please select the appliances you wish to add to the integration. '
            input(name: 'goveeApp', type: 'enum', required:false, description: 'Please select the appliances you wish to add to the integration.', multiple:true,
                options: options2, width: 8, height: 1)
        }
    }
}

def sceneExtract() {
    def sceneData = [:]

    logger('sceneExtract() DEVICE INFORMATION', 'debug')
    if (state.goveeHomeToken) {
    goveeHomeToken = "Bearer " + state.goveeHomeToken
    

    def params = [
            uri   : 'https://app2.govee.com',
            path  : '/bff-app/v1/exec-plat/home',
            headers: ['Authorization': goveeHomeToken, 'Content-Type': 'application/json', 'appVersion': '5.6.01'],
        ]
        logger("sceneExtract(): Calling HTTP server", 'error')
    try {
        httpGet(params) { resp ->
            def slurper = new JsonSlurper()
//            parsed  = slurper.parse(resp.data)
//            logger("sceneExtract(): ${parsed}", 'error')
                resp.data.data.components.forEach {
                    name = it.name.toString()
//                    logger("sceneExtract(): ${name}", 'error')
                    if (name == 'Default') {
                        logger("sceneExtract(): found default group moving forward", 'debug')
                        it.oneClicks.iotRules.forEach {
                            devName= it.deviceObj.name
                            devSku= it.deviceObj.sku.get(0)
                            logger("sceneExtract(): Looking at ${devName} ${devSku}", 'info')
                            rules = it.rule
                            if (rules.get(0).get(0).cmdType == 0) {
                                logger("sceneExtract(): First rule is on/off as expected cmdtype is ${rules.get(0).get(0).cmdType}", 'debug')
                                cmdVal=rules.get(0).get(1).cmdVal
                                iotMsg=rules.get(0).get(1).iotMsg
                            } else {
                                logger("sceneExtract(): First rule is not on/off as expected cmdtype is ${rules.get(0).get(0).cmdType}", 'debug')
                                cmdVal=rules.get(0).get(0).cmdVal
                                iotMsg=rules.get(0).get(0).iotMsg    
                            }
                            iotMsgJson  = slurper.parseText(iotMsg)
                            cmdValJson  = slurper.parseText(cmdVal)
                            if ( cmdValJson.scenesStr == null) {
                                sceneName = cmdValJson.diyName }
                            else {
                                sceneName = cmdValJson.scenesStr
                            }
                            command = iotMsgJson.msg.data.command
                            logger("sceneExtract(): rule is ${rules}", 'trace')
                            logger("sceneExtract(): cmdVal is ${cmdVal}", 'trace')
                            logger("sceneExtract(): iotMSG is ${iotMsg}", 'trace')
                            logger("sceneExtract(): cmdValJson is ${cmdValJson}", 'trace')
                            logger("sceneExtract(): iotMsgJson is ${iotMsgJson}", 'trace')
                            logger("sceneExtract(): Scene Name is ${sceneName}: command is ${command}", 'debug')
                            logger("sceneExtract(): Scene Name is ${sceneName}: command is ${command.inspect().replaceAll("\'", "\"")}", 'debug')
                            diyAdd(devSku, sceneName, command)
/*                            state.lightEffects.H6066.each {
                            logger("sceneExtract(): Show all scenes in application ${it.getKey()}", 'debug')
                            logger("sceneExtract(): Show all scenes in application ${it.value}", 'debug')
                            } */
                           }
                    } else {
//                    logger("sceneExtract(): Default group not found", 'error')
                    } 
                }
        }
    } catch (groovyx.net.http.HttpResponseException e) {
        logger("deviceSelect() Error: e.statusCode ${e.statusCode}", 'error')
        logger("deviceSelect() ${e}", 'error')

        return 'unknown'
    }
    }
    dynamicPage(name: 'sceneExtract', title: 'Scene Extract', uninstall: false, install: false, submitOnChange: true)
    {
        section('Govee Home Creds')
        {
            paragraph "Please provide your gove home token."
            href 'sceneExtract2', title: 'Govee Home Creds', description: 'Setup Gove Home Creds to obtain token'
            if (timeNow > state.goveeHomeExpiry) {
            paragraph "Token has expired. Please click the Govee HOme Creds button above to refresh"
            } else {
                paragraph "Token has valid"
            }
        }
        section('Extracted command below:') {
            paragraph "Device name ${devName}"
            paragraph "Scene name is ${sceneName}"
            paragraph "Command is ${command.inspect().replaceAll("\'", "\"")}" 
            paragraph "This command will work with any device with model ${devSku}" 
        }
    }
}

def sceneExtract2() {
    
    logger('sceneExtract2() DEVICE INFORMATION', 'debug')
    if (settings.goveeEmail && settings.goveePassword) {
//        if (timeNow > state.goveeHomeExpiry) {
            bodyParm = '{"email": "'+settings.goveeEmail+'", "password": "'+settings.goveePassword+'"}'
            logger("sceneExtract(): bodyparm to be passed:  ${bodyParm}", 'error')
            def params = [
                uri   : 'https://community-api.govee.com',
                path  : '/os/v1/login',
                headers: ['Content-Type': 'application/json'],
                body: bodyParm
            ]
            logger("sceneExtract(): parms to be passed:  ${params}", 'error')
            try {
                httpPost(params) { resp ->
                    logger("sceneExtract(): response is ${resp.data}", 'error')
                    status = resp.data.status
                    msg = resp.data.message
                    logger("sceneExtract(): status is ${status}: Message ${msg}", 'error')
                    if (status == 200) {
                        state.goveeHomeToken = resp.data.data.token
                        state.goveeHomeExpiry = resp.data.data.expiredAt.value
                        logger("sceneExtract(): response is ${resp.data}", 'error')
                        logger("sceneExtract(): token is ${state.goveeHomeToken} and expires at ${state.goveeHomeExpiry}", 'error')
                    } else {
                        logger("sceneExtract(): Login failed check error above and correct", 'info')
                        }
                    }
                } catch (groovyx.net.http.HttpResponseException e) {
                    logger("deviceSelect() Error: e.statusCode ${e.statusCode}", 'error')
                    logger("deviceSelect() ${e}", 'error')

                return 'unknown'
            }        
 //       }
    }           
            
    dynamicPage(name: 'sceneExtract2', title: 'Govee Home Credentials', uninstall: false, install: false, submitOnChange: true)
    {
        section('Govee Home Creds')
        {
            paragraph "Please provide your gove home token."
            input 'goveeEmail', 'string', title: 'Please enter your Email used to log into Govee Home', required: true
            input 'goveePassword', 'password', title: 'Please enter your Govee Home password', required: true
            if (settings.goveeEmail && settings.goveePassword) {
                if (status == 200) {
                paragraph "Login Successful"
                } else {
                    paragraph "Login failed with error code ${status}: ${msg}"
                }
            }
        }
    }
}


def about() {
    dynamicPage(name: 'about', title: 'About Govee Integration with HE', uninstall: false, install: false)
    {
        section()
        {
            paragraph image: 'https://lh4.googleusercontent.com/-1dmLp--W0OE/AAAAAAAAAAI/AAAAAAAAEYU/BRuIXPPiOmI/s0-c-k-no-ns/photo.jpg', 'Govee Integration'
        }
        section('Support the Project')
        {
            paragraph 'Govee is provided free for personal and non-commercial use.  I have worked on this app in my free time to fill the needs I have found for myself and others like you.  I will continue to make improvements where I can. If you would like you can donate to continue to help with development please use the link below.'
            href(name: 'donate', style:'embedded', title: "Consider making a \$5 or \$10 donation today to support my ongoing effort to continue improving this integration.", url: 'https://www.paypal.me/mavrrick58')
        }
    }
}

def installed() {
    log.debug "Installed with settings: ${settings}"
    state?.isInstalled = true
    state?.diyEffects = [:]
    initialize()
}

def updated() {
    log.debug "Updated with settings: ${settings}"
    initialize()
    if (state?.diyEffects == null) {
        state?.diyEffects = [:]    
    }
}

def uninstalled() {
    // external cleanup. No need to unsubscribe or remove scheduled jobs
    // 1.4 Remove dead virtual devices
    getChildDevices()?.each
    { childDevice ->
            deleteChildDevice(childDevice.deviceNetworkId)
    }
}

def DeviceInfo() {
    def options = [:]
    log.debug 'DEVICE INFORMATION'
    def params = [
            uri   : 'https://developer-api.govee.com',
            path  : '/v1/devices',
            headers: ['Govee-API-Key': settings.APIKey, 'Content-Type': 'application/json'],
        ]

    try {
        httpGet(params) { resp ->
                //List each device assigned to current API key
                //log.debug resp.data
                resp.data.data.devices.each {
                    deviceID = it.device
                    deviceModel = it.model
                    deviceName = it.deviceName
                    controllable = it.controllable
                    retrievable = it.retrievable
                    ctMin = it.properties.colorTem.range.min
                    ctMax = it.properties.colorTem.range.max
                    log.debug "$deviceName    Address: $deviceID    Model: $deviceModel retrievable: $retrievable controllable: $controllable Color Temp: $ctMin Color Temp: $ctMax"
                    options.put('deviceID', deviceID)
                    options.put('Name', deviceName)
                    log.debug "$options"
                }
                return resp.data
                return options
        }
    } catch (groovyx.net.http.HttpResponseException e) {
        log.error "Error: e.statusCode ${e.statusCode}"
        log.error "${e}"

        return 'unknown'
    }
}

def apiRateLimits(type, value) {
    logger("apiRateLimits($type, $value)", 'info')
    if (type == 'DailyLimitRemaining') {
        state.dailyLimit = value
        if ( state.dailyLimit.toInteger() < apiV1threshold) {
            sendnotification('Govee Lights, Plugs, Switches APi Rate Limit', state.dailyAppLimit)
        }
    }
    else if (type == 'DailyLimitRemainingV2') {
        state.dailyAppLimit = value
        log.debug "${state.dailyAppLimit}"
        if ( state.dailyAppLimit.toInteger() < apiV2threshold) {
            log.debug 'validated api limit to low. Sending notificatoin'
            sendnotification('Govee Appliance API rate Limit', state.dailyAppLimit)
        }
    }
}

def sendnotification (type, value) {
    if (notifyEnabled) {
        String notificationText = "${type} has reached threshold of ${value}. You will have issues if this get to zero"
        notificationDevice?.each {
            it.deviceNotification(notificationText)
        }
    }
}

/* def getParentSetting(String settingName) {
    return parent?."${settingName}"
} */

def diyAdd(devSKU, diyName, command) {
    logger("diyAdd(): Attempting add DIY Scene ${devSKU}:${diyName}:${command}", 'trace')
    command = command.inspect().replaceAll("\'", "\"")
    diyEntry = [:]
    diyEntry.put("name", diyName)
    diyEntry.put("cmd", command)
    logger("diyAdd(): Trying to add ${diyEntry}", 'debug')
    logger("diyAdd(): keys are  ${state.diyEffects.keySet()}", 'debug')
    if (state.diyEffects.containsKey(devSKU) == false) {
        logger("diyAdd(): Device ${devSKU} not found", 'debug')
        logger("diyAdd(): New Device. Starting at 1001", 'debug')
        diyAddNum = 1001
        diyEntry2 = [:]
        diyEntry2.put(diyAddNum,diyEntry)
        state.diyEffects.put(devSKU,diyEntry2)
    } else {
        compare = state.diyEffects."${devSKU}".toString()
        matchVal = compare.indexOf(diyName)
        if (matchVal > 0) {
            logger("diyAdd(): Scene with same name already present", 'debug')
            } else {            
            logger("diyAdd(): Device ${devSKU} was found. Adding Scene to existing scene list", 'debug')
            diySize = state.diyEffects."${devSKU}".size()
            diyAddNum = (diySize + 1001).toInteger()
            logger("diyAdd(): Current DiY size is ${diySize}", 'debug')
            state.diyEffects."${devSKU}".put(diyAddNum,diyEntry)
        }
    }
}




/**
 *  logger()
 *
 *  Wrapper function for all logging.
 **/
private logger(msg, level = 'debug') {
    switch (level) {
        case 'error':
            if (state.loggingLevelIDE >= 1) { log.error msg }
            break

        case 'warn':
            if (state.loggingLevelIDE >= 2)  { log.warn msg }
            break

        case 'info':
            if (state.loggingLevelIDE >= 3) { log.info msg }
            break

        case 'debug':
            if (state.loggingLevelIDE >= 4) { log.debug msg }
            break

        case 'trace':
            if (state.loggingLevelIDE >= 5) { log.trace msg }
            break

        default:
            log.debug msg
            break
    }
}

private def appButtonHandler(button) {
    if (button == "sceneInitialize") {
        log.debug "Initializing Scene data"
        state?.lightEffects = [:]
        lightEffectSetup()
    } else if (button == "pushScenesUpdate") {
        List child = getChildDevices()
        child.each {
        logger('initialize() API key has been updated. Calling child devices to udpate', 'debug')
        it.initialize()
        } 
    } else if (button == "sceneDIYInitialize") {
        state?.diyEffects = [:] 
    }    
}


